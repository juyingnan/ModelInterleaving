package model.bigraph;

public class Constraint {
	private String name;
	private Sorting pre;
	private Sorting next;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Sorting getPre() {
		return pre;
	}
	public void setPre(Sorting pre) {
		this.pre = pre;
	}
	public Sorting getNext() {
		return next;
	}
	public void setNext(Sorting next) {
		this.next = next;
	}
	
}
