package model.service.petrinet;

import java.util.ArrayList;

public class PetrinetModel {
	private StartPlace start;
	private ArrayList<Place> placeList;
	private ArrayList<TimedTransition> transitionList;
	private ArrayList<Arc> arcList;
	
	public StartPlace getStart() {
		return start;
	}
	public void setStart(StartPlace start) {
		this.start = start;
	}
	public ArrayList<Place> getPlaceList() {
		return placeList;
	}
	public void setPlaceList(ArrayList<Place> placeList) {
		this.placeList = placeList;
	}
	public ArrayList<TimedTransition> getTransitionList() {
		return transitionList;
	}
	public void setTransitionList(ArrayList<TimedTransition> transitionList) {
		this.transitionList = transitionList;
	}
	public ArrayList<Arc> getArcList() {
		return arcList;
	}
	public void setArcList(ArrayList<Arc> arcList) {
		this.arcList = arcList;
	}
}
