package model.configInfo;

public class ConfigInfo {
	private String ServerIP = null;
	private String BigraphModelFilePath = null;
	private String BigraphDataModelFilePath = null;
	private String EFSMModelFilePath = null;
	private String PetrinetModelFilePath = null;
	private String ServiceModelFilePath = null;
	private String BGMFilePath = null;
	private String SortingConstraintPath = null;
	private int BigraphServerPort = 0;
	private int ServiceServerPort = 0;
	private String PatternsOutputPath = null;
	private String TestCaseOutputPath = null;
	private String TomcatRunPath = null;
	private String OutputByBigMC=null;
	private String DefinePathOutputPath = null;

	public String getServerIP() {
		return ServerIP;
	}

	public void setServerIP(String serverIP) {
		ServerIP = serverIP;
	}
	
	public String getBigraphModelFilePath() {
		return BigraphModelFilePath;
	}

	public void setBigraphModelFilePath(String bigraphModelFilePath) {
		this.BigraphModelFilePath = bigraphModelFilePath;
	}

	public String getBigraphDataModelFilePath() {
		return BigraphDataModelFilePath;
	}

	public void setBigraphDataModelFilePath(String bigraphDataModelFilePath) {
		BigraphDataModelFilePath = bigraphDataModelFilePath;
	}

	public String getEFSMModelFilePath() {
		return EFSMModelFilePath;
	}

	public void setEFSMModelFilePath(String eFSMModelFilePath) {
		this.EFSMModelFilePath = eFSMModelFilePath;
	}

	public String getPetrinetModelFilePath() {
		return PetrinetModelFilePath;
	}

	public void setPetrinetModelFilePath(String petrinetModelFilePath) {
		PetrinetModelFilePath = petrinetModelFilePath;
	}

	public String getServiceModelFilePath() {
		return ServiceModelFilePath;
	}

	public void setServiceModelFilePath(String serviceModelFilePath) {
		this.ServiceModelFilePath = serviceModelFilePath;
	}
	
	public int getBigraphServerPort() {
		return BigraphServerPort;
	}

	public void setBigraphServerPort(int bigraphServerPort) {
		this.BigraphServerPort = bigraphServerPort;
	}

	public int getServiceServerPort() {
		return ServiceServerPort;
	}

	public void setServiceServerPort(int serviceServerPort) {
		this.ServiceServerPort = serviceServerPort;
	}

	public String getSortingConstraintPath() {
		return SortingConstraintPath;
	}

	public void setSortingConstraintPath(String sortingConstraintPath) {
		this.SortingConstraintPath = sortingConstraintPath;
	}

	public String getBGMFilePath() {
		return BGMFilePath;
	}

	public void setBGMFilePath(String bGMFilePath) {
		BGMFilePath = bGMFilePath;
	}

	public String getPatternsOutputPath() {
		return PatternsOutputPath;
	}

	public void setPatternsOutputPath(String patternsOutputPath) {
		this.PatternsOutputPath = patternsOutputPath;
	}

	public String getTestCaseOutputPath() {
		return TestCaseOutputPath;
	}

	public void setTestCaseOutputPath(String testCaseOutputPath) {
		TestCaseOutputPath = testCaseOutputPath;
	}

	public String getTomcatRunPath() {
		return TomcatRunPath;
	}

	public void setTomcatRunPath(String tomcatRunPath) {
		TomcatRunPath = tomcatRunPath;
	}

	public String getOutputByBigMC() {
		return OutputByBigMC;
	}

	public void setOutputByBigMC(String outputByBigMC) {
		OutputByBigMC = outputByBigMC;
	}

	public String getDefinePathOutputPath() {
		return DefinePathOutputPath;
	}

	public void setDefinePathOutputPath(String definePathOutputPath) {
		DefinePathOutputPath = definePathOutputPath;
	}

	public void printConfigInfo(){
		System.out.println("*****************ConfigInfo***********************");
		System.out.println("Server IP:" + this.ServerIP);
		System.out.println("Bigraph Model:" + this.BigraphModelFilePath);
		System.out.println("Bigraph Server Port:" + this.BigraphServerPort);
		System.out.println("Petrinet Model:" + this.PetrinetModelFilePath);
		System.out.println("Service Model:" + this.ServiceModelFilePath);
		System.out.println("Service Server Port:" + this.ServiceServerPort);
		System.out.println("EFSM Model:" + this.EFSMModelFilePath);
		System.out.println("Patterns Output:" + this.PatternsOutputPath);
		System.out.println("BGM File Path:" + this.BGMFilePath);
		System.out.println("Sorting Constraint:" + this.SortingConstraintPath);
		System.out.println("Test Data Output:" + this.TestCaseOutputPath);
		System.out.println("Tomcat Run Path:" + this.TomcatRunPath);
		System.out.println("Test Path by BigMC:" + this.OutputByBigMC);
		System.out.println("Define Path Output:" + this.DefinePathOutputPath);
	}
}
